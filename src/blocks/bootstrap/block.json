{
    "apiVersion": 2,
    "name": "devaqsa-gutenberg-blocks/bootstrap",
    "version": "0.1.0",
    "title": "Bootstrap Block",
    "category": "devaqsa-gutenberg-blocks",
    "icon": "smiley",
    "description": "Example block written with ESNext standard and JSX support build step required.",
    "supports": {
        "html": false,
        "anchor": true
    },
    "attributes": {
        "content": {
            "type": "string",
            "default": "Hello World!"
        },
        "color": {
            "type": "string",
            "default": "#00ff00"
        },
        "margin": {
            "type": "string",
            "default": "m-3"
        },
        "padding": {
            "type": "string",
            "default": "p-3"
        },
        "alignment": {
            "type": "string",
            "default": "text-start"
        },
        "displayClass": {
            "type": "string",
            "default": "d-block"
        },
        "backgroundColor": {
            "type": "string",
            "default": ""
        },
        "containerType": {
            "type": "string",
            "default": ""
        },
        "flexProperties": {
            "type": "string",
            "default": ""
        },
        "borderClass": {
            "type": "string",
            "default": ""
        },
        "spacing": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "devaqsa-gutenberg-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}